{
  "dependencies": ["PerformanceCalcs"],
  "static": [
    {
      "formula": "f_energy",
      "args": {
        "outputs": {"ac_power": "Pac", "timeseries": "timeseries"}
      },
      "returns": ["hourly_energy", "hourly_timeseries"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"freq": "MONTHLY"},
        "outputs": {"items": "hourly_energy", "timeseries": "hourly_timeseries"}
      },
      "returns": ["monthly_energy"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"freq": "YEARLY"},
        "outputs": {"items": "hourly_energy", "timeseries": "hourly_timeseries"}
      },
      "returns": ["annual_energy"]
    }
  ]
}
