{
  "dependencies": ["performance"],
  "static": [
    {
      "formula": "f_rollup",
      "args": {
        "data": {"intervals": "hours"},
        "outputs": {"integrand": "Pac", "timeseries": "timeseries"}
      },
      "returns": ["HourlyEnergy"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"intervals": "months"},
        "outputs": {"integrand": "HourlyEnergy", "timeseries": "timeseries"}
      },
      "returns": ["MonthlyEnergy"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"intervals": "years"},
        "outputs": {"integrand": "HourlyEnergy", "timeseries": "timeseries"}
      },
      "returns": ["AnnualEnergy"]
    }
  ]
}
