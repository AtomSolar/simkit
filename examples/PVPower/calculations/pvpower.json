{
  "dependencies": ["performance"],
  "static": [
    {
      "formula": "f_energy",
      "args": {
        "outputs": {"ac_power": "Pac", "timeseries": "timeseries"}
      },
      "returns": ["energy"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"freq": "months"},
        "outputs": {"items": "HourlyEnergy", "timeseries": "timeseries"}
      },
      "returns": ["MonthlyEnergy"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"freq": "years"},
        "outputs": {"items": "HourlyEnergy", "timeseries": "timeseries"}
      },
      "returns": ["AnnualEnergy"]
    }
  ]
}
