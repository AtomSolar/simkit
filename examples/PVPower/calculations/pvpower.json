{
  "static": [
    {
      "formula": "f_total_irrad",
      "args":{
        "data": {
          "surface_tilt": "surface_tilt", "surface_azimuth": "surface_azimuth",
          "model": "irradiance_model"
        },
        "outputs": {
          "solar_zenith": "solar_zenith", "solar_azimuth": "solar_azimuth",
          "dni": "dni", "ghi": "ghi", "dhi": "dhi",
          "extraterrestrial": "extraterrestrial", "am_abs": "am_abs"
        }
      },
      "returns": ["poa_global", "poa_direct", "poa_diffuse"]
    },
    {
      "formula": "f_cell_temp",
      "args": {
        "data": {"wind_speed": "Uwind", "air_temp": "Tamb"},
        "output": {"poa_global": "poa_global"}
      },
      "returns": ["Tcell"]
    },
    {
      "formula": "f_dc_power",
      "args": {
        "data": {"module": "module"},
        "output": {
          "poa_direct": "poa_direct", "poa_diffuse": "poa_diffuse",
          "cell_temp": "Tcell", "am_abs": "am_abs", "aoi": "aoi"
        }
      },
      "returns": ["Isc", "Imp", "Voc", "Vmp", "Pmp", "Ee"]
    },
    {
      "formula": "f_ac_power",
      "args": {
        "data": {"inverter": "inverter"},
        "output": {"v_mp": "Vmp", "p_mp": "Pmp"}
      },
      "returns": ["Pac"]
    }
  ]
}
