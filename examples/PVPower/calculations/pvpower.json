{
  "dependencies": ["performance"],
  "static": [
    {
      "formula": "f_energy",
      "args": {
        "outputs": {"ac_power": "Pac", "timeseries": "timeseries"}
      },
      "returns": ["hourly_energy", "hourly_timeseries"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"freq": "months"},
        "outputs": {"items": "HourlyEnergy", "timeseries": "timeseries"}
      },
      "returns": ["monthly_energy"]
    },
    {
      "formula": "f_rollup",
      "args": {
        "data": {"freq": "years"},
        "outputs": {"items": "HourlyEnergy", "timeseries": "timeseries"}
      },
      "returns": ["annual_energy"]
    }
  ]
}
