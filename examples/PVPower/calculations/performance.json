{
  "dependencies": ["IrradianceCalcs"],
  "static": [
    {
      "formula": "f_aoi",
      "args": {
        "data": {"surface_tilt": "latitude", "surface_azimuth": "surface_azimuth"},
        "outputs": {"solar_zenith": "solar_zenith", "solar_azimuth": "solar_azimuth"}
      },
      "returns": ["aoi"]
    },
    {
      "formula": "f_cell_temp",
      "args": {
        "data": {"wind_speed": "Uwind", "air_temp": "Tamb"},
        "outputs": {"poa_global": "poa_global"}
      },
      "returns": ["Tcell", "Tmod"]
    },
    {
      "formula": "f_dc_power",
      "args": {
        "data": {"module": "module"},
        "outputs": {
          "poa_direct": "poa_direct", "poa_diffuse": "poa_diffuse",
          "cell_temp": "Tcell", "am_abs": "am_abs", "aoi": "aoi"
        }
      },
      "returns": ["Isc", "Imp", "Voc", "Vmp", "Pmp", "Ee"]
    },
    {
      "formula": "f_ac_power",
      "args": {
        "data": {"inverter": "inverter"},
        "outputs": {"v_mp": "Vmp", "p_mp": "Pmp"}
      },
      "returns": ["Pac"]
    }
  ]
}
